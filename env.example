# E*TRADE API Credentials
# Get these from: https://us.etrade.com/etx/ris/apikey
ETRADE_CONSUMER_KEY=your_consumer_key_here
ETRADE_CONSUMER_SECRET=your_consumer_secret_here

# Use sandbox environment (true/false)
# Set to false for production
ETRADE_SANDBOX=true

# E*TRADE Login Credentials (for automated OAuth flow)
# These are used by the agent via Playwright MCP to automate the authorization process
ETRADE_USERNAME=your_username_here
ETRADE_PASSWORD=your_password_here

# Optional: Custom token storage file path
# ETRADE_TOKEN_FILE=.etrade_tokens.json

